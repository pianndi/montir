{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.1"
    }
  },
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/public/css"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js"
    },
    {
      "src": "/(favicon\\.ico|robots\\.txt|style\\.css)",
      "dest": "/public/$1"
    },
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_NAME": "Bengkel Mobil",
    "APP_ENV": "production",
    "APP_KEY": "base64:RsOG9xyrS+0hYpiyutsfDaRlfdb+zmpw3+phOAEqXs0=",
    "APP_DEBUG": "false",
    "APP_TIMEZONE": "UTC",
    "APP_URL": "https://montir.vercel.app",
    "DB_CONNECTION": "sqlite",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  },
  "outputDirectory": "public"
}